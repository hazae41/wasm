{
  "type": "module",
  "name": "@hazae41/wasm",
  "version": "1.1.11",
  "description": "WebAssembly Binary Format (.wasm) decoder-modifier-encoder for TypeScript",
  "repository": "github:hazae41/wasm",
  "author": "hazae41",
  "license": "MIT",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "rm -rf ./out && tsc && tscousin"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "dependencies": {
    "@hazae41/binary": "^2.0.2",
    "@hazae41/cursor": "^2.1.3"
  },
  "devDependencies": {
    "@hazae41/phobos": "^2.0.16",
    "@hazae41/tscousin": "^1.0.28",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "binary",
    "encoder",
    "decoder",
    "parsing",
    "editing",
    "encoding",
    "decoding",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
}